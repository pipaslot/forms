{
  "name": "pipaslot/forms",
  "description": "Nette form extension with Bootstrap rendering",
  "keywords": [
    "nette",
    "pipas",
    "bootstrap",
    "tools",
    "forms"
  ],
  "license": "LGPL-3.0",
  "authors": [
    {
      "name": "Petr Å tipek",
      "email": "pipaslot@seznam.cz"
    }
  ],
  "require": {
    "nette/di": "~2.3",
    "nette/forms": "~2.3",
    "nette/utils": "~2.2",
    "nette/application": "~2.3",
    "latte/latte": "^2.4"
  },
  "require-dev": {
    "pipaslot/modules": "~1.0",
    "composer/composer": "~1.0@dev",
    "nette/nette": "~2.3",
    "nette/tester": "~1.3",
    "tracy/tracy": "^2.3",
    "nette/bootstrap": "~2.3",
    "nette/robot-loader": "~2.3"
  },
  "autoload": {
    "classmap": [
      "src/"
    ]
  },
  "autoload-dev": {
    "classmap": [
      "demo/"
    ]
  },
  "extra": {
    "directory": {
      "create": [
        "temp",
        "demo/log",
        "demo/temp"
      ],
      "empty": [
        "temp",
        "demo/temp"
      ],
      "secure": [
      ]
    },
    "bower": {
      "files": [
        "bower.json"
      ]
    },
    "media": {
    },
    "grunt": [
    ]
  },
  "scripts": {
    "post-install-cmd": [
      "Pipas\\Modules\\Composer\\EventReceiver::run"
    ],
    "post-update-cmd": [
      "Pipas\\Modules\\Composer\\EventReceiver::run"
    ]
  }
}
